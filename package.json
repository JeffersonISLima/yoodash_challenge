{
  "name": "yoodash-challenge",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "prisma:generate": "DATABASE_URL=${DATABASE_URL:-postgresql://postgres:postgres@localhost:5433/yoodash?schema=public} prisma generate",
    "prisma:migrate": "DATABASE_URL=${DATABASE_URL:-postgresql://postgres:postgres@localhost:5433/yoodash?schema=public} prisma migrate dev",
    "prisma:deploy": "DATABASE_URL=${DATABASE_URL:-postgresql://postgres:postgres@localhost:5433/yoodash?schema=public} prisma migrate deploy",
    "prisma:studio": "DATABASE_URL=${DATABASE_URL:-postgresql://postgres:postgres@localhost:5433/yoodash?schema=public} prisma studio",
    "lint": "echo 'No linter configured'"
  },
  "dependencies": {
    "@fastify/swagger": "^8.14.1",
    "@fastify/swagger-ui": "^3.0.2",
    "@prisma/client": "^6.18.0",
    "date-fns": "^4.1.0",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^1.1.9",
    "pg": "^8.13.1",
    "prisma": "^6.18.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "@types/pg": "^8.15.6",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
